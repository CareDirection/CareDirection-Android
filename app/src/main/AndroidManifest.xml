<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.caredirection">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".product.detail.ActivityProductDetail"></activity>
        <activity android:name=".product.standard.ActivityProductStandard" />
        <activity android:name=".care_product.CareRegisterComplete" />
        <activity android:name=".MainActivity" />
        <activity android:name=".care_product.CareProductActivity" />
        <activity android:name=".research.ResearchChange" />
        <activity android:name=".research.userInfo.ResearchSymptomActivity" />
        <activity android:name=".research.userInfo.ResearchDiseaseActivity" />
        <activity android:name=".research.userInfo.ResearchGenderActivity" />
        <activity android:name=".home.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".research.userInfo.ResearchNameActivity"></activity>

        <service android:name=".common.fcm.CadiFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>