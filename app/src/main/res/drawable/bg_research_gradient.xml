<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">
            <gradient
                android:type="radial"
                android:gradientRadius="200%p"
                android:centerX="0.3"
                android:centerY="0"
                android:startColor="@color/colorYellow"
                android:endColor="#cee6af"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:type="radial"
                android:gradientRadius="200%p"
                android:centerX="0.2"
                android:centerY="0.1"
                android:startColor="#cee6af"
                android:centerColor="#5ebfab"
                android:endColor="@color/colorPrimaryDark"/>
        </shape>
    </item>
</layer-list>